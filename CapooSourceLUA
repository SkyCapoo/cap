local tt = {0x63C30, 0x158, 0xB8, 0x1B4}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = 1, freeze = true}})
gg.toast("緩降開啟成功")
end

