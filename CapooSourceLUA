function playerhitdamge()
attack = gg.prompt({"输入攻擊力:"}
,{[1]=1000},{[1]="number"})[1]
gg.clearResults()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1A0, 0x1E0, 0x188}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = attack}})
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1A0, 0x1E0, 0x18c}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = attack}})
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1A0, 0x1E0, 0x190}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = attack}})
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1A0, 0x1E0, 0x194}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 16, value = attack}})
gg.toast("攻擊力修改成功")
end

