function teleport()
mnsj={}
mnsj.qq=gg.prompt({"要自己解开冻结横向坐标","高度","竖向坐标"},mnsj.qqa)if mnsj.qq then 
mnsj.qqa=mnsj.qq
xc1=mnsj.qq[1].."00"
xc2=mnsj.qq[2].."00"
xc3=mnsj.qq[3].."00"
--x
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x198, 0xBC}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = xc1}})
--y
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x198, 0xC0}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = xc2}})
--z
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x198, 0xC4}
local ttt = S_Pointer(t, tt, true)

gg.setValues({{address = ttt, flags = 4, value = xc3}})
gg.toast("定點傳送修改成功")
end
end





