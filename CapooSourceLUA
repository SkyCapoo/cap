function hitbox()
	 gg.clearResults()
	 gg.setRanges(4)
	 gg.searchNumber("-1F;-1F;60;180;0;120;220;120::29", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
	 gg.searchNumber("120;220;120", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
	 jg=gg.getResults(100)
	 sl=gg.getResultCount()
	 for i = 1, sl do
	 	 dzy=jg[i].address
	 	 gg.setValues({[1] = {address = dzy,flags = gg.TYPE_DWORD,freeze = false,value = 1500}})
	 end
	 gg.toast("HITBOX已修改數值")
	 gg.alert("HITBOX已修改數值")
	 gg.clearResults()
end


function infjump1()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x198, 0x84}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 4, value = 1, freeze = true}})
gg.toast("連跳開啟成功")
end

function infjump2()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x198, 0x84}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 4, value = 1, freeze = false}})
gg.toast("連跳關閉成功")
end


function fly1()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x168}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 4, value = 8, freeze = true}})
gg.toast("飛天開啟成功")
end

function fly2()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x168}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = 0}})
gg.addListItems({{address = ttt, flags = 4, value = 8, freeze = false}})
gg.setValues({{address = ttt, flags = 4, value = 0}})
gg.toast("飛天關閉成功")
end


function nopflytask1()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x170}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 4, value = -1, freeze = true}})
gg.toast("飛天過檢測開啟成功")
