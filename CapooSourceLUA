function MountsUI() 
xg2= gg.choice({
    "☯️第一格坐騎添加/修改☯️",
    "☯️第二格坐騎添加/修改☯️",
    "☯️第三格坐騎添加/修改☯️",
    "☯️第四格坐騎添加/修改☯️",   
    "☯️第五格坐騎添加/修改☯️",
    " ☯️🔷 ✖️返 回 ✖️🔷☯️ ",
},2018,os.date("格數坐騎添加"))
if xg2==1 then xgms="第一格坐騎" MountUIID1() end--第一格坐騎
if xg2==2 then xgms="第二格坐騎" MountUIID2() end---第二格坐騎
if xg2==3 then xgms="第三格坐騎" MountUIID3() end---第三格坐騎
if xg2==4 then xgms="第四格坐騎" MountUIID4() end---第四格坐騎
if xg2==5 then xgms="第五格坐騎" MountUIID5() end----第五格坐騎
XGCK=-1
end

function itemschangehack()
XY = gg.prompt({"请输入要改的物品代码\n输入12239恢复"},{[1]=10500},{[1]="number"})[1]
gg.toast(so({"噴漆改物",12240,4,4},{{200,-0x1B0,4},{2,0x8,4}},{{XY,-0x280,4,1}})) gg.toast("噴漆修改成功")end


function setworldtoolitemblocks()
x = gg.prompt({"请输入你要改的编辑器填充物代码,三个祭台140,141,142,三个圣遗物587,588,589,卡图方块1049,老版本炸药桶835,刷怪方块1080"},{[1]=835},{[1]="number"})[1]
gg.searchNumber("104", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()--搜索值
gg.refineNumber("104", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()--移动值
gg.refineNumber("104", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()--移动值
gg.refineNumber("104", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()--移动值
revert = gg.getResults(10000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.editAll(x, gg.TYPE_DOUBLE)
revert = gg.getResults(10000, nil, nil, nil, nil, nil, nil, nil, nil)--修改值
gg.editAll(x, gg.TYPE_DOUBLE)--修改值

gg.clearResults()--退出值
gg.processResume()--退出值
gg.toast("填充物修改成功")
end


ikun2=0
function autodohead1()
ai2 = -180
repeat
ai2 = ai2 + AA3s2
gg.sleep(20) 
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230,0x368,0x88}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai2, freeze = true}})
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230,0x250,0x50}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai2, freeze = true}})
until (ai2 > 180)
if (ikun2 >0) then
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230,0x368,0x88}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai2, freeze = false}})
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230,0x250,0x50}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = ai2, freeze = false}})
return
else
return autodohead1()
end end

function autodohead2()
ikun2=1

gg.toast("轉頭旋轉關閉成功")
return
end



