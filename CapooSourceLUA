function b()
if gg.getTargetInfo()["label"] ~="Mini World" then
gg.alert("❗Error進程錯誤")
print("❗Error進程錯誤")
gg.toast("❗Error進程錯誤")
else
gg.alert("✔️進程正確")
print("✔️進程正確")
gg.toast("✔️進程正確")
end
end
