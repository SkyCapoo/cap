local tt = {0x264230, 0x1C8, 0x168}
local ttt = S_Pointer(t, tt, true)
gg.setValues({{address = ttt, flags = 4, value = 0}})
gg.addListItems({{address = ttt, flags = 4, value = 8, freeze = false}})
gg.setValues({{address = ttt, flags = 4, value = 0}})
gg.toast("飛天關閉成功")
end


