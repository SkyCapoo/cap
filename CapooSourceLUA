function sizexyz()
playersize = gg.prompt({"输入模型大小（1是正常大小）"}
,{[1]=5},{[1]="number"})[1]
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230,0x1A0,0x320}
local ttt = S_Pointer(t, tt,true)
gg.setValues({{address = ttt, flags = 16, value = playersize, freeze = true}})
gg.toast("人物大小修改成功")
end
function invisible1()
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x198, 0x12C}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = 1000, freeze = true}})
gg.sleep(1800)
local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230, 0x1C8, 0x198, 0x12C}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = 1000, freeze = false}})
gg.setValues({{address = ttt, flags = 16, value = -7}})
gg.sleep(10)

local t = {"liblibGameApp.so", "Cd"}
local tt = {0x63C30, 0x158, 0xB8, 0x1B4}
local ttt = S_Pointer(t, tt, true)
gg.addListItems({{address = ttt, flags = 16, value = 104716760.0, freeze = true}})

local t = {"liblibGameApp.so:bss", "Cb"}
local tt = {0x264230,0x1A0,0x320}
local ttt = S_Pointer(t, tt,true)
gg.setValues({{address = ttt, flags = 16, value = -240, freeze = true}})
gg.toast("隱身開啟成功")
end


